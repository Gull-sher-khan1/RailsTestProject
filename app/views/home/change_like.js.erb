<%puts from%>
<%if from == :create%>
  document.getElementById("like_buttons_<%=post.id%>").innerHTML = '<%= escape_javascript render partial: "home/liked", locals:{post:post, like_id: like_id} %>';
<%else%>
  document.getElementById("like_buttons_<%=post.id%>").innerHTML = '<%= escape_javascript render partial: "home/like", locals:{post:post} %>';
<%end%>
